/[ \t]/ {}
/[0-9]+/  { lval.n, _= strconv.Atoi(yylex.Text()); return ID }
/(\.[A-Za-z]+)+/ { lval.nodes = yylex.Text(); return FIELD }
/(p|print)/ { return INSPECT}
/(h|help)/ {return HELP}
/(q|uit)/ {return QUIT}
/(l|list)/ {return LIST}
/./ {return int(yylex.Text()[0])}
